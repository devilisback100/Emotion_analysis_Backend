{
    "version": 2,
    "builds": [
        {
            "src": "./index.py",
            "use": "@vercel/python",
            "config": { "runtime": "python3.9" }
        }
    ],
    "routes": [
        {
            "src": "/classify_emotion",
            "dest": "/classify_emotion"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
